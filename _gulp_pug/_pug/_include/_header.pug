// Header
header.header
  .header__top
    .container

      .header__top-wrapper
        div
          .header__catalog
            button(type="button").header__catalog-btn.j-btn
              i.ion-navicon-round
              | Каталог
            nav.header__catalog-nav.j-btn-target
              ul
                li: a(href="#!") Автоэлектрика
                li: a(href="#!") Крепления для автомобилей 
                li: a(href="#!") Двигатель и компоненты 
                li: a(href="#!") Стеклоочистители
                li: a(href="#!") Инструменты 
                li: a(href="#!") Материалы для установки шин 
                li: a(href="#!") Топливная система 
                li: a(href="#!") Коробка передач и шасси 
                li: a(href="#!") Фильтры
        div
          .header__menu
            button(type="button").header__menu-btn.j-btn Меню сайта
            nav.header__menu-nav.j-btn-target
              ul
                li: a(href="#!") О компании
                li: a(href="#!") Гарантия и сертификаты
                li: a(href="#!") Где купить
                li: a(href="#!") Партнерам
                li: a(href="#!") Новости
                li: a(href="#!") Контакты
